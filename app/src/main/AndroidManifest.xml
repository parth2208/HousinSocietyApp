<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.housingsocietyapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.UI.UI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".View.UI.UI.ProfileActivity"/>
        <activity android:name=".View.UI.UI.SearchActivity"/>
        <activity android:name=".View.UI.UI.NoticeAcivity"/>
        <activity android:name=".View.UI.UI.PaymentsActivity"/>
        <activity android:name=".View.UI.UI.SubmitPaymentActivity"/>
        <activity android:name=".View.UI.UI.VendorActivity"/>
        <activity android:name=".View.UI.UI.ComplaintActivity"/>
        <activity android:name=".View.UI.UI.SocietyRules"/>


    </application>

</manifest>